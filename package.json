{
    "name": "cohesive",
    "version": "0.6.0",
    "description": "Fast, Functional and Modular JavaScript library designed for a modern web.",
    "homepage": "http://github.com/cohesiveio/cohesive.js",
    "keywords":[ "cohesive", "cohesive.io", "closure", "es6" ],
    "main":"./dist/cjs/node.js",
    "directories": {
        "lib": "./dist/cjs/",
