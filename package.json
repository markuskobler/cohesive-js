    "devDependencies": {
        "es6-module-transpiler": "0.8.x",
        "promises-aplus-tests": "2.0.5",
        "jshint": "2.5.x",
        "mocha": "1.21.x",
        "chai": "1.9.x",
        "chai-as-promised": "4.1.x",
        "istanbul": "0.3.x",
        "karma": "0.12.x",
        "karma-mocha": "0.1.x",
        "karma-chai": "0.1.x",
        "karma-coverage": "0.2.x",
        "karma-teamcity-reporter": "0.1.x",
        "karma-chrome-launcher": "0.1.x",
        "karma-firefox-launcher": "0.1.x",
        "karma-phantomjs-launcher": "0.1.x",
        "karma-sauce-launcher": "0.2.x",
        "closurecompiler": "1.3.x",
        "uglify-js":"2.4.15"
    },
